{
  "expo": {
    "name": "capitals-app",
    "slug": "capitals-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "capitalsapp",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A2E40"
      },
      "userInterfaceStyle": "dark"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#1A2E40"
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}